#include <bits/stdc++.h>

using namespace std;

void gradingStudents(int grades[],int n) {
    for(int i=0; i<n; i++){
        int temp= grades[i];
        int add= grades[i]%5;
        if(temp<38){
            grades[i]+=0;
        }
        else if ((5-add)==2 || (5-add)==1) {
            grades[i]=grades[i]+(5-add);
        }
        else if (add==0) {
            grades[i]+=0;
        }
        else {
        grades[i]+=0;
        }
    }
    for (int j=0; j<n; j++){
        cout<<grades[j]<<endl;
    }
}

int main(){
    int n;
    cin>>n;
    int arr[n];
    for (int i=0; i<n; i++){
        cin>>arr[i];
    }
    gradingStudents(arr, n);
}
